[PATH]
config=./networks/Deep_N4.spec
load=./experiments/Deep_N4/network/
data=./dataset/fish/spec/batch
save=./experiments/Deep_N4/network/

[OPTIMIZE]
n_threads=34
force_fft=0
optimize_fft=1

[TRAIN]
train_range=1-3
test_range=0
outsz=80,80,1
dp_type=volume
cost_fn=binomial_cross_entropy
data_aug=1
softmax=1

[UPDATE]
force_eta=0.01
anneal_factor=0.997
anneal_freq=100
momentum=0.9
wc=0
minibatch=1
norm_grad=0
rebalance=0

[MONITOR]
n_iters=1000000
check_freq=10
test_freq=100
test_samples=10

[SCAN]
scanner=volume
offset=0,0,0
dim=0,0,0
weight_idx=0
outname=out
